{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$", 
        "include": ["."],                       
        "exclude": ["node_modules"]             
    },
    "recurseDepth": 10,                         
    "opts": {
        "template": "node_modules/docdash",
        "destination": "./docs/",               
        "recurse": true                         
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
      },
    "docdash": {
        "sort": false,
        "search": true,
        "collapse": false,
        "sectionOrder": [
        "Namespaces",
        "Classes"
   ]
  }
}